expression: expr. 
-expr: term; sum; diff.
sum: term, "+", term+"+".
diff: term, "-", term+"-".
-term: factor; prod; div.
prod: factor, "×", factor+"×".
div: factor, "÷", factor+"÷".
-factor: name; number; bracketed.
bracketed: "(", expr, ")".
name: ["a"-"z"]+.
number: ["0"-"9"]+.
#
234×(bbbb+cccc+dddd)×12

#

<expression>
   <prod>
      <number>234</number>×
      <bracketed>(
         <sum>
            <name>bbbb</name>+
            <name>cccc</name>+
            <name>dddd</name>
         </sum>)</bracketed>×
      <number>12</number>
   </prod>
</expression>
