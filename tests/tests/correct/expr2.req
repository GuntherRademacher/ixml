expression: expr. 
-expr: term; sum; diff.
sum: term, -"+", term+-"+".
diff: term, -"-", term+-"-".
-term: factor; prod; div.
prod: factor, -"×", factor+-"×".
div: factor, -"÷", factor+-"÷".
-factor: name; number; -"(", ^expr, -")".
name: ["a"-"z"]+.
number: ["0"-"9"]+.
#
234×(bbbb+cccc+dddd)×12#

<expression>
   <prod>
      <number>234</number>
      <expr>
         <sum>
            <name>bbbb</name>
            <name>cccc</name>
            <name>dddd</name>
         </sum>
      </expr>
      <number>12</number>
   </prod>
</expression>
