expression: expr.
-expr: term; sum; diff.
sum: expr, -"+", term.
diff: expr, "-", term.
-term: factor; prod; div.
prod: term, -"ร", factor.
div: term, "รท", factor.
-factor: id; number; bracketed.
bracketed: -"(", expr, -")".
id: @name.
name: letter+.
number: digit+.
-letter: ["a"-"z"].
-digit: ["0"-"9"].
#
(3)
#

<expression>
   <bracketed>
      <number>3</number>
   </bracketed>
</expression>
