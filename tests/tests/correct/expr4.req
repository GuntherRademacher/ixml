expression: expr. 
-expr: term; sum.
sum: term, "+", term+"+".
-term: factor; prod.
prod: factor, "×", factor+"×".
-factor: id; number; "(", expr, ")".
id: ["a"-"z"]+.
number: ["0"-"9"]+, (".", ["0"-"9"]+)?.
#
pi+(10×a×b)+3.14
#

<expression>
   <sum>
      <id>pi</id>+(
      <prod>
         <number>10</number>×
         <id>a</id>×
         <id>b</id>
      </prod>)+
      <number>3.14</number>
   </sum>
</expression>
